{
	"name": "deep-research-devin",
	"version": "1.0.0",
	"description": "Deep research CLI with OpenAI Responses API",
	"license": "MIT",
	"bin": {
		"deep-research": "./source/cli.tsx"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"start": "bun run source/cli.tsx",
		"dev": "bun run source/cli.tsx",
		"build": "tsc",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "^2.0.0",
		"dotenv": "^16.4.5",
		"ink": "^4.1.0",
		"meow": "^11.0.0",
		"openai": "^4.58.1",
		"react": "^18.2.0",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/node": "^20.14.0",
		"@types/react": "^18.0.32",
		"@types/yargs": "^17.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"bun-types": "^1.1.18",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
